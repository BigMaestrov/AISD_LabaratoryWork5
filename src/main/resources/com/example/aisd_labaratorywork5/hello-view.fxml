<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="529.0" prefWidth="700.0" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.aisd_labaratorywork5.HelloController">

    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <TitledPane animated="false" prefHeight="75.0" prefWidth="660.0" text="Data">
     <content>
       <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="53.0" prefWidth="658.0">
            <children>
               <TextField fx:id="input" prefHeight="50.0" prefWidth="660.0" />
            </children>
         </AnchorPane>
     </content>
   </TitledPane>
   <ButtonBar buttonMinWidth="100.0" prefHeight="40.0" prefWidth="700.0">
     <buttons>
         <Button fx:id="startButton" mnemonicParsing="false" onAction="#clickAdd" prefHeight="40.0" prefWidth="70.0" text="add" />
         <Button mnemonicParsing="false" onAction="#clickDelete" prefHeight="40.0" prefWidth="400.0" text="delete" />
     </buttons>
   </ButtonBar>
   <AnchorPane prefHeight="95.0" prefWidth="660.0">
      <children>
         <TitledPane animated="false" prefHeight="77.0" prefWidth="660.0" text="LIFO">
           <content>
             <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="44.0" prefWidth="658.0">
                  <children>
                     <TextField fx:id="lifotext" prefHeight="50.0" prefWidth="660.0" />
                  </children>
               </AnchorPane>
           </content>
         </TitledPane>
      </children>
   </AnchorPane>
   <TitledPane animated="false" text="FIFO">
     <content>
       <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="50.0" prefWidth="658.0">
            <children>
               <TextField fx:id="fifotext" layoutY="-1.0" prefHeight="77.0" prefWidth="660.0" />
            </children>
         </AnchorPane>
     </content>
   </TitledPane>

    <Label fx:id="welcomeText" />
</VBox>
